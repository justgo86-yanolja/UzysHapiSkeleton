{
  "name": "uzyshapiskeleton",
  "version": "0.1.0",
  "description": "Hapi.js Application Skeleton",
  "main": "app.js",
  "scripts": {
    "start": "./node_modules/.bin/pm2 start app.js -i max",
    "test": "lab -C -c -m 2000",
    "debug": "export NODE_ENV=development &&nodemon --debug -e js,html app.js --type=development",
    "stop": "./node_modules/.bin/pm2 stop app.js",
    "coverage" : "if test -f './converage.html'; then  rm coverage.html; fi && lab -r html -o coverage.html",
    "dep": "npm list --depth 0",
    "outdated": "npm outdated --depth 0",
    "jshint": "./node_modules/jshint/bin/jshint *.js src test;true",
    "inspector":"./node_modules/node-inspector/bin/inspector.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uzysjung/UzysHapiSkeleton.git"
  },
  "keywords": [
    "Uzys",
    "Hapi",
    "Hapijs",
    "Hapi.js",
    "Node",
    "Node.js",
    "Skeleton"
  ],
  "engines" : { "node" : ">=5.0.0" },
  "dependencies": {
    "hapi": "^11.1.2",
    "vision": "^4.0.1",
    "hapi-swagger": "^2.2.4",
    "boom": "^3.0.0",
    "good": "^6.4.0",
    "good-console": "^5.2.0",
    "pm2": "^0.15.10",
    "tv": "5.0.1",
    "joi": "^7.0.1",
    "debug":"2.2.x",
    "async" : "*",
    "request" : "*",
    "mysql" : "^2.9.x",
    "underscore": "^1.8.3",
    "underscore.string": "^3.1.1",
    "bcrypt": "^0.8.5",
    "hapi-auth-basic":"^3.0.0",
    "handlebars":"^3.0.3",
    "knex": "^0.8.6",
    "inert": "^3.2.0"
  },
  "devDependencies": {
    "code": "^2.0.1",
    "lab": "^7.3.0",
    "node-inspector": "^0.12.3",
    "jshint":"^2.8.x",
    "nodemon":"^1.8.x"
  },
  "author": "Uzysjung",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uzysjung/UzysHapiSkeleton/issues"
  },
  "homepage": "https://github.com/uzysjung/UzysHapiSkeleton#readme"
}
